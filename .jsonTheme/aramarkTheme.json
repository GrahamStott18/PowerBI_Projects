{
    "name": "Aramark Theme",
    "$schema": "reportThemeSchema-2.114.json", 
  
    "firstLevelElements": "#000000",
    "secondLevelElements": "#EB002A",
    "thirdLevelElements": "#EBEBEB",
    "fourthLevelElements": "#98999B",
    "background": "#EBEBEB",
    "secondaryBackground": "#54565A",
    "tableAccent": "#118DFF", 
  
    "textClasses": {
      "label": { "fontFace": "Calibri", "fontSize": 12 },
      "callout": { "fontFace": "Calibri" },
      "title": { "fontFace": "Calibri", "fontSize": 16 },
      "header": { "fontFace": "Calibri" }
    },
  
  
    "dataColors": [
      "#98999B",
      "#0070C0",
      "#00FF00",
      "#000000",
      "#EBEBEB",
      "#FFF2CC",
      "#EB002A",
      "#54565A",
      "#252423"
    ],
  
  
    "bad": "#9C0006",
    "neutral": "#9C6500",
    "good": "#006100",
    "foreground": "#000000",
    "foregroundNeutralSecondary": "#54565A",
    "backgroundLight": "#EBEBEB",
    "foregroundNeutralTertiary": "#98999B",
    "backgroundNeutral": "#54565A",
  
  
    "visualStyles": {
        "*": {
          "*": {
            "background": [
              {
                "color": { "solid": { "color": "#EBEBEB" } },
                "transparency": 15
              }
            ],
            "border": [ 
              {
                "show": false
              }
            ],
            "visualHeader": [
              {
                "background": { "solid": { "color": "#FFFFFF" } }
              }
            ] 
          }
        },
  
  
      "page": {
        "*": {
          "background": [
            { "color": { "solid": { "color": "#FFFFFF" } }, "transparency": 100 }
          ]
        }
      }
    }
  }  